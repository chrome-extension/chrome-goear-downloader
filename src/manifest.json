{
  "name": "__MSG_goear_downloader_title__",
  "short_name": "GoearDown",
  "description": "__MSG_goear_downloader_description__",
  "default_locale": "en",
  "manifest_version": 2,
  "version": "0.4.0",
  "permissions": [
    "*://*.goear.com/listen/*"
  ],
  "icons": {
    "48" : "logo-48.png",
    "128" : "logo-128.png"
  },
  "content_scripts": [{
      "matches": ["*://*.goear.com/listen/*"],
      "js" : ["download.js"]
    }]
}
